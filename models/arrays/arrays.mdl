{UTF-8}
inputA[DimA]=
	-1 ,
	+2 ,
	3
	~
	~	constant array
	|

a[DimA] = inputA[DimA] * 10
	~
	~	array to array
	|

b[DimA] = 42
	~
	~		~	:SUPPLEMENTARY
	|

c[DimA] = inputA[DimA] + a[DimA]
	~
	~	adding arrays
	~	:SUPPLEMENTARY
	|

d[A1] = inputA[A1] * 10
	~
	~	element to element
	~	:SUPPLEMENTARY
	|

e[DimB] = inputA[DimA] * 10
	~
	~	mapped array
	|

inputAB[A1,B1] = 11 ~~|
inputAB[A1,B2] = 12 ~~|
inputAB[A1,B3] = 13 ~~|
inputAB[A2,B1] = 14 ~~|
inputAB[A2,B2] = 15 ~~|
inputAB[A2,B3] = 16 ~~|
inputAB[A3,B1] = 17 ~~|
inputAB[A3,B2] = 18 ~~|
inputAB[A3,B3] = 19
	~
	~		|

f[DimA, DimB] = inputAB[DimA, DimB] * a[DimA]
	~
	~	2-d array
	~	:SUPPLEMENTARY
	|

g[DimB] = INTEG(a[DimA], e[DimB])
	~
	~	array of levels
	~	:SUPPLEMENTARY
	|

h = SUM(a[DimA!]) + 1
	~
	~	sum an array
	~	:SUPPLEMENTARY
	|

o[DimB] = SUM(inputAB[DimA!, DimB])
	~
	~	sum a 2D array over columns
	~	:SUPPLEMENTARY
	|

p[DimA] = SUM(inputAB[DimA, DimB!])
	~
	~	sum a 2D array over rows
	~	:SUPPLEMENTARY
	|

r[DimA,DimB] = inputAB[DimA,DimB] * g[DimB] ~~|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 1
	~	Month
	~	The final time for the simulation.
	|

INITIAL TIME  = 0
	~	Month
	~	The initial time for the simulation.
	|

SAVEPER  =
        TIME STEP
	~	Month [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 1
	~	Month [0,?]
	~	The time step for the simulation.
	|
